{
  "name": "phonotate",
  "version": "1.0.0",
  "main": "src/main.js",
  "permissions": {
    "media": true
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "cross-env NODE_ENV=development webpack --mode development && electron .",
    "build": "cross-env NODE_ENV=development webpack --mode development",
    "build:bundle": "cross-env NODE_ENV=production webpack --mode=production",
    "package": "npm run build:bundle && electron-builder"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "babel-loader": "^9.2.1",
    "cross-env": "^7.0.3",
    "electron": "^34.0.0",
    "electron-builder": "^25.1.8",
    "electron-packager": "^17.1.2",
    "electron-rebuild": "^3.2.9",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "better-sqlite3": "^11.7.2",
    "bootstrap": "^5.3.3",
    "diff": "^7.0.0",
    "electron-is-dev": "^3.0.1",
    "path-browserify": "^1.0.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.1",
    "uuid": "^11.0.5"
  },
  "build": {
    "appId": "app.phonotate",
    "productName": "PhonotateApp",
    "directories": {
      "output": "release-build"
    },
    "asar": false,
    "files": [
      "./dist/**/*",
      "src/**/*",
      "index.html",
      "images/**/*",
      "css/**/*",
      "!**/data",
      "!**/*.map",
      "!**/data/*.db",
      "!**readme/**/*"
    ],
    "win": {
      "target": "nsis"
    }
  }
}